{
  "name": "game-of-life",
  "version": "0.6.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "react-scripts": "3.4.3",
    "redux": "4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:all": "npm run test -- --watchAll=false",
    "test:ci": "start-server-and-test start 3000 cypress:run:record",
    "eject": "react-scripts eject",
    "preversion": "npm run test:all",
    "version": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file && git add CHANGELOG.md",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record --key $CYPRESS_DASHBOARD_KEY",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "26.0.14",
    "conventional-changelog": "3.1.23",
    "cypress": "5.3.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "redux-devtools-extension": "2.13.8",
    "start-server-and-test": "1.11.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:all"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
